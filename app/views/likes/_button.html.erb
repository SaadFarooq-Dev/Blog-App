<div class="like-button-parital">
  <small class="px-1"> <%= likeable.likes.count %> Likes </small>
  <% if user_signed_in? %>
    <% like = current_user.likes.find_by(likeable: likeable) %>
  <% end %>
  <% if like.nil? %>  <%= button_to("Like", likes_path,  class:"btn btn-success", remote: user_signed_in?,  params:{ like: {likeable_id: likeable.id,  likeable_type: likeable.class.to_s}}) %>
  <% else %>
    <%= button_to("Unlike", like_path(like) ,class:"btn btn-success ",  method: :delete, remote: user_signed_in?) %>
  <% end %>
</div>
